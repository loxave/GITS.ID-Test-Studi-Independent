package StudiIndependent.gits;

import java.util.Scanner;

public class test4 {

    public static void main(String[] args) {

        String  reverse =  "";
        String reverseBack = "";
        System.out.println("text here: ");

        Scanner dataInput = new Scanner(System.in);
        reverse = dataInput.nextLine();

        int res = reverse.length();
        for (int i = res - 1; i>= 0; i--){
            reverseBack = reverseBack + reverse.charAt(i);
        }

        System.out.println("Kebalikan dari " + reverse + "is " + reverseBack);
      
    }
}
